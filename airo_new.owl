@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix airo: <https://w3id.org/airo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@base <https://w3id.org/airo#> .

<https://w3id.org/airo> rdf:type owl:Ontology ;
                         owl:versionIRI <https://w3id.org/airo/2.8> ;
                         rdfs:label "AI 风险本体 (Full Definition & No Event Version)"@zh-cn ;
                         terms:description "此版本为最终完整定义版。移除了'AI事件'和'prov:Activity'。'时间'与'报道'直接关联至'风险'或'活动'。所有概念均包含基于权威标准的中文定义。"@zh-cn .

#################################################################
#    1. 核心顶级活动 (Core Processes - Top Level)
#################################################################

###  https://w3id.org/airo#AI生命周期活动
airo:AI生命周期活动 rdf:type owl:Class ;
                   rdfs:label "AI生命周期活动"@zh-cn ;
                   rdfs:comment "指AI系统从概念形成、开发、部署到退役过程中执行的任何技术或管理过程。替代了原有的静态'生命周期阶段'概念 (来源: ISO/IEC 5338 AI system life cycle processes)。"@zh-cn .

###  https://w3id.org/airo#风险管理活动
airo:风险管理活动 rdf:type owl:Class ;
                 rdfs:label "风险管理活动"@zh-cn ;
                 rdfs:comment "针对AI系统风险进行识别、分析、评价和应对的协调活动，旨在指导和控制组织关于风险的决策。替代了原有的'风险控制'概念 (来源: ISO 31000 Risk management)。"@zh-cn .

#################################################################
#    2. 时间与空间要素 (Time & Space - Top Level)
#################################################################

###  https://w3id.org/airo#使用地点
airo:使用地点 rdf:type owl:Class ;
             rdfs:label "使用地点"@zh-cn ;
             rdfs:comment "AI系统部署、运行的具体物理场所（如工厂、医院）、地理区域（如欧盟、中国）或逻辑网络位置 (来源: EU AI Act)。"@zh-cn .

###  https://w3id.org/airo#时间要素
airo:时间要素 rdf:type owl:Class ;
             rdfs:label "时间要素 (时间线)"@zh-cn ;
             rdfs:comment "用于描述风险显现或活动执行的时间属性集合，构成时间线的基础。"@zh-cn .

###  https://w3id.org/airo#发生时间
airo:发生时间 rdf:type owl:Class ;
             rdfs:subClassOf airo:时间要素 ;
             rdfs:label "发生时间"@zh-cn ;
             rdfs:comment "风险开始显现、事故发生或活动开始执行的具体时间点或日期 (来源: ISO/IEC 27035)。"@zh-cn .

###  https://w3id.org/airo#发现时间
airo:发现时间 rdf:type owl:Class ;
             rdfs:subClassOf airo:时间要素 ;
             rdfs:label "发现时间"@zh-cn ;
             rdfs:comment "风险、异常或事故首次被人类、系统识别、检测到或被媒体公开报道的时间点 (来源: ISO/IEC 27035)。"@zh-cn .

###  https://w3id.org/airo#持续时间
airo:持续时间 rdf:type owl:Class ;
             rdfs:subClassOf airo:时间要素 ;
             rdfs:label "持续时间"@zh-cn ;
             rdfs:comment "从风险/活动开始到结束、解决或闭环之间的时间跨度 (来源: ISO 8601)。"@zh-cn .

#################################################################
#    3. 描述与来源 (Description & Sources - Top Level)
#################################################################

###  https://w3id.org/airo#详细描述
airo:详细描述 rdf:type owl:Class ;
             rdfs:subClassOf airo:文档 ;
             rdfs:label "详细描述"@zh-cn ;
             rdfs:comment "对AI风险、新闻报道或活动过程的清晰、结构化文本概述，用于记录核心事实摘要 (来源: Dublin Core)。"@zh-cn .

###  https://w3id.org/airo#信息来源
airo:信息来源 rdf:type owl:Class ;
             rdfs:subClassOf airo:文档 ;
             rdfs:label "信息来源"@zh-cn ;
             rdfs:comment "提供风险或活动信息的原始出处，如新闻链接、技术报告、白皮书或政府公告 (来源: ISO/IEC 27000)。"@zh-cn .

###  https://w3id.org/airo#报道机构
airo:报道机构 rdf:type owl:Class ;
             rdfs:subClassOf airo:利益相关者 ;
             rdfs:label "报道机构"@zh-cn ;
             rdfs:comment "发布AI相关新闻、调查报告或风险预警的媒体、研究机构或非政府组织。"@zh-cn .

###  https://w3id.org/airo#新闻报道
airo:新闻报道 rdf:type owl:Class ;
             rdfs:subClassOf airo:文档 ;
             rdfs:label "新闻报道"@zh-cn ;
             rdfs:comment "描述风险或活动的具体媒体文章、广播内容或网络帖子。"@zh-cn .

#################################################################
#    4. 风险类扩展 (Risk Extensions)
#################################################################

###  https://w3id.org/airo#风险分类
airo:风险分类 rdf:type owl:Class ;
             rdfs:subClassOf airo:风险概念 ;
             rdfs:label "风险分类"@zh-cn ;
             rdfs:comment "根据风险的性质、来源、后果或影响领域对风险进行的结构化分组（例如：技术鲁棒性风险、基本权利风险） (来源: ISO 31000 Risk Taxonomy)。"@zh-cn .

###  https://w3id.org/airo#风险等级
airo:风险等级 rdf:type owl:Class ;
             rdfs:subClassOf airo:风险概念 ;
             rdfs:label "风险等级"@zh-cn ;
             rdfs:comment "风险的幅度或重要性，通常根据后果的严重性和发生的可能性的组合来确定（如：高、中、低、极高） (来源: ISO Guide 73)。"@zh-cn .

#################################################################
#    5. 子类层级结构 (Subclasses)
#################################################################

# --- AI生命周期活动的子类 ---

airo:数据治理活动 rdf:type owl:Class ;
                 rdfs:subClassOf airo:AI生命周期活动 ;
                 rdfs:label "数据治理活动"@zh-cn ;
                 rdfs:comment "与数据收集、准备、清洗、评估和管理相关的活动，旨在确保数据质量和合规性 (来源: AI Act Art. 10)。"@zh-cn .

airo:数据偏差检测与纠正 rdf:type owl:Class ;
                       rdfs:subClassOf airo:数据治理活动 ;
                       rdfs:label "数据偏差检测与纠正"@zh-cn ;
                       rdfs:comment "专门检测并纠正数据集中可能影响健康、安全或基本权利的偏差的活动 (来源: AI Act Art. 10(2)(f))。"@zh-cn .

airo:AI模型开发活动 rdf:type owl:Class ;
                   rdfs:subClassOf airo:AI生命周期活动 ;
                   rdfs:label "AI模型开发活动"@zh-cn ;
                   rdfs:comment "构建、训练和优化 AI 模型的活动，包括模型设计、特征工程和参数拟合 (来源: ISO/IEC 5338)。"@zh-cn .

airo:验证与确认活动 rdf:type owl:Class ;
                   rdfs:subClassOf airo:AI生命周期活动 ;
                   rdfs:label "验证与确认活动"@zh-cn ;
                   rdfs:comment "确保系统符合规格要求（验证）并满足用户预期用途（确认）的测试活动 (来源: AI Act Art. 15)。"@zh-cn .

airo:技术文档编制 rdf:type owl:Class ;
                 rdfs:subClassOf airo:AI生命周期活动 ;
                 rdfs:label "技术文档编制"@zh-cn ;
                 rdfs:comment "创建、更新和维护合规性技术文档的活动，以证明符合法律要求 (来源: AI Act Art. 11)。"@zh-cn .

airo:记录保存活动 rdf:type owl:Class ;
                 rdfs:subClassOf airo:AI生命周期活动 ;
                 rdfs:label "记录保存活动"@zh-cn ;
                 rdfs:comment "系统运行期间自动记录事件日志的活动，用于可追溯性 (来源: AI Act Art. 12)。"@zh-cn .

airo:上市后监控活动 rdf:type owl:Class ;
                   rdfs:subClassOf airo:AI生命周期活动 ;
                   rdfs:label "上市后监控活动"@zh-cn ;
                   rdfs:comment "系统投放市场后，主动收集和分析性能及风险数据的持续活动 (来源: AI Act Art. 72)。"@zh-cn .

airo:严重事件报告 rdf:type owl:Class ;
                 rdfs:subClassOf airo:上市后监控活动 ;
                 rdfs:label "严重事件报告"@zh-cn ;
                 rdfs:comment "向监管机构报告导致人员伤亡或基础设施中断的严重事件的活动 (来源: AI Act Art. 73)。"@zh-cn .

# --- 风险管理活动的子类 ---

airo:风险识别 rdf:type owl:Class ;
             rdfs:subClassOf airo:风险管理活动 ;
             rdfs:label "风险识别"@zh-cn ;
             rdfs:comment "发现、识别和描述风险源及其潜在后果的过程 (来源: ISO 31000)。"@zh-cn .

airo:风险分析 rdf:type owl:Class ;
             rdfs:subClassOf airo:风险管理活动 ;
             rdfs:label "风险分析"@zh-cn ;
             rdfs:comment "理解风险性质并确定风险等级（结合可能性与严重性）的过程 (来源: ISO 31000)。"@zh-cn .

airo:风险评价 rdf:type owl:Class ;
             rdfs:subClassOf airo:风险管理活动 ;
             rdfs:label "风险评价"@zh-cn ;
             rdfs:comment "将风险分析结果与风险准则进行比较，以确定风险是否可接受的过程 (来源: ISO 31000)。"@zh-cn .

airo:风险应对 rdf:type owl:Class ;
             rdfs:subClassOf airo:风险管理活动 ;
             rdfs:label "风险应对"@zh-cn ;
             rdfs:comment "选择并实施措施以修正风险（如降低、消除、转移）的过程 (来源: ISO 31000)。"@zh-cn .

airo:风险审查与监控 rdf:type owl:Class ;
                   rdfs:subClassOf airo:风险管理活动 ;
                   rdfs:label "风险审查与监控"@zh-cn ;
                   rdfs:comment "对风险管理过程的持续监控和定期审查，以确保有效性 (来源: ISO 31000)。"@zh-cn .

#################################################################
#    6. 基础类 (Basic Entities) - 完整定义
#################################################################

airo:AI能力 rdf:type owl:Class ;
           rdfs:label "AI能力"@zh-cn ;
           rdfs:comment "AI 系统实现其目的所需的技术能力或功能特性 (来源: ISO/IEC 22989)。"@zh-cn .

airo:AI组件 rdf:type owl:Class ;
           rdfs:label "AI组件"@zh-cn ;
           rdfs:comment "构成 AI 系统功能的要素，包括软件、硬件、数据或算法 (来源: ISO/IEC 22989)。"@zh-cn .

airo:AI部署者 rdf:type owl:Class ;
             rdfs:subClassOf airo:AI运营者 ;
             rdfs:label "AI部署者"@zh-cn ;
             rdfs:comment "任何在其职权下使用 AI 系统的自然人、法人、公共机构或其他团体（个人非职业活动除外） (来源: EU AI Act)。"@zh-cn .

airo:AI开发者 rdf:type owl:Class ;
             rdfs:subClassOf airo:利益相关者 ;
             rdfs:label "AI开发者"@zh-cn ;
             rdfs:comment "涉及 AI 服务和产品开发、编码或设计的组织或实体 (来源: ISO/IEC 22989)。"@zh-cn .

airo:AI模型 rdf:type owl:Class ;
           rdfs:subClassOf airo:AI组件 ;
           rdfs:label "AI模型"@zh-cn ;
           rdfs:comment "基于输入数据生成推理或预测的物理或数学结构 (来源: ISO/IEC TR 24028)。"@zh-cn .

airo:AI运营者 rdf:type owl:Class ;
             rdfs:subClassOf airo:利益相关者 ;
             rdfs:label "AI运营者"@zh-cn ;
             rdfs:comment "指提供者、产品制造商、部署者、授权代表、进口商或分销商等对系统负有责任的实体 (来源: EU AI Act)。"@zh-cn .

airo:AI提供者 rdf:type owl:Class ;
             rdfs:subClassOf airo:AI运营者 ;
             rdfs:label "AI提供者"@zh-cn ;
             rdfs:comment "开发 AI 系统，或拥有 AI 系统并以其名义投放市场或投入使用的自然人、法人或公共机构 (来源: EU AI Act)。"@zh-cn .

airo:AI对象 rdf:type owl:Class ;
           rdfs:subClassOf airo:利益相关者 ;
           rdfs:label "AI对象"@zh-cn ;
           rdfs:comment "受 AI 使用影响或作为 AI 处理对象的实体（例如被面部识别扫描的人） (来源: AIRO)。"@zh-cn .

airo:AI系统 rdf:type owl:Class ;
           rdfs:label "AI系统"@zh-cn ;
           rdfs:comment "一种基于机器的系统，设计为以不同程度的自主性运行，可能在部署后表现出适应性，并为显式或隐式目标，从收到的输入中推断如何生成预测、内容、推荐或决策等输出 (来源: EU AI Act Art 3(1))。"@zh-cn .

airo:AI技术 rdf:type owl:Class ;
           rdfs:label "AI技术"@zh-cn ;
           rdfs:comment "系统开发中使用的方法或技术，如机器学习、逻辑编程或知识库方法 (来源: ISO/IEC 22989)。"@zh-cn .

airo:AI用户 rdf:type owl:Class ;
           rdfs:subClassOf airo:利益相关者 ;
           rdfs:label "AI用户"@zh-cn ;
           rdfs:comment "与系统直接交互的个人或团体 (来源: ISO/IEC TR 24028)。"@zh-cn .

airo:影响领域 rdf:type owl:Class ;
             rdfs:label "影响领域"@zh-cn ;
             rdfs:comment "受 AI 系统影响的领域或方面，例如身体健康、隐私权、民主进程或环境 (来源: AIRO)。"@zh-cn .

airo:自动化水平 rdf:type owl:Class ;
               rdfs:label "自动化水平"@zh-cn ;
               rdfs:comment "指 AI 系统运行的自主程度，范围从辅助人类到完全自主 (来源: ISO/IEC 22989)。"@zh-cn .

airo:变更 rdf:type owl:Class ;
         rdfs:label "变更"@zh-cn ;
         rdfs:comment "实体（如系统、数据、环境）中的变动或修改，可能触发新的风险评估 (来源: AIRO)。"@zh-cn .

airo:行为准则 rdf:type owl:Class ;
             rdfs:label "行为准则"@zh-cn ;
             rdfs:comment "一套非约束性的规范和责任，用于指导 AI 的合规开发和使用 (来源: EU AI Act)。"@zh-cn .

airo:后果 rdf:type owl:Class ;
         rdfs:subClassOf airo:风险概念 ;
         rdfs:label "后果"@zh-cn ;
         rdfs:comment "影响目标的风险事件的直接结果 (来源: ISO 31000)。"@zh-cn .

airo:数据 rdf:type owl:Class ;
         rdfs:subClassOf airo:AI组件 ;
         rdfs:label "数据"@zh-cn ;
         rdfs:comment "指数据集及广泛的数据概念，包括训练数据、测试数据和验证数据 (来源: ISO/IEC 20546)。"@zh-cn .

airo:文档 rdf:type owl:Class ;
         rdfs:label "文档"@zh-cn ;
         rdfs:comment "记录了信息的任何介质，包括文本、图表或电子记录 (来源: ISO 15489)。"@zh-cn .

airo:技术文档 rdf:type owl:Class ;
             rdfs:subClassOf airo:文档 ;
             rdfs:label "技术文档"@zh-cn ;
             rdfs:comment "包含系统设计、开发、测试和验证细节的合规性文档，用于证明符合法规要求 (来源: AI Act Annex IV)。"@zh-cn .

airo:领域 rdf:type owl:Class ;
         rdfs:label "领域"@zh-cn ;
         rdfs:comment "与经济活动相关的区域、部门或行业，如医疗、金融、交通 (来源: NACE Codes)。"@zh-cn .

airo:频率 rdf:type owl:Class ;
         rdfs:label "频率"@zh-cn ;
         rdfs:comment "单位时间内事件或结果发生的数量 (来源: ISO 31073)。"@zh-cn .

airo:通用AI模型 rdf:type owl:Class ;
               rdfs:subClassOf airo:AI模型 ;
               rdfs:label "通用AI模型"@zh-cn ;
               rdfs:comment "表现出显著通用性，能胜任广泛任务并可集成到下游系统中的 AI 模型 (GPAI) (来源: EU AI Act)。"@zh-cn .

airo:硬件平台 rdf:type owl:Class ;
             rdfs:label "硬件平台"@zh-cn ;
             rdfs:comment "系统运行所在的物理硬件设施，如 CPU、GPU、TPU (来源: AIRO)。"@zh-cn .

airo:危险源 rdf:type owl:Class ;
           rdfs:subClassOf airo:风险源 ;
           rdfs:label "危险源"@zh-cn ;
           rdfs:comment "潜在的伤害来源 (Hazard) (来源: ISO 31073)。"@zh-cn .

airo:人类介入 rdf:type owl:Class ;
             rdfs:label "人类介入"@zh-cn ;
             rdfs:comment "指示人类在 AI 运行过程中的监督（Human-in-the-loop）或操作 (来源: EU AI Act Art 14)。"@zh-cn .

airo:影响 rdf:type owl:Class ;
         rdfs:subClassOf airo:后果 ;
         rdfs:label "影响"@zh-cn ;
         rdfs:comment "后果对个人、群体、社会、环境等造成的最终结果 (Impact) (来源: ISO 31000)。"@zh-cn .

airo:输入 rdf:type owl:Class ;
         rdfs:label "输入"@zh-cn ;
         rdfs:comment "系统或组件生成输出所需的输入数据、信号或指令 (来源: AIRO)。"@zh-cn .

airo:许可证 rdf:type owl:Class ;
           rdfs:label "许可证"@zh-cn ;
           rdfs:comment "代表版权或许可使用协议 (来源: SPDX)。"@zh-cn .

airo:可能性 rdf:type owl:Class ;
           rdfs:label "可能性"@zh-cn ;
           rdfs:comment "某事发生的概率或几率 (Likelihood) (来源: ISO 31073)。"@zh-cn .

airo:模态 rdf:type owl:Class ;
         rdfs:label "模态"@zh-cn ;
         rdfs:comment "系统存在或被表现的形式（如文本、图像、音频、视频） (来源: AIRO)。"@zh-cn .

airo:输出可控性模式 rdf:type owl:Class ;
                   rdfs:label "输出可控性模式"@zh-cn ;
                   rdfs:comment "人类对 AI 输出行使控制的级别或方式 (来源: AIRO)。"@zh-cn .

airo:输出 rdf:type owl:Class ;
         rdfs:label "输出"@zh-cn ;
         rdfs:comment "系统生成的结果、预测、内容或决策 (来源: AIRO)。"@zh-cn .

airo:目的 rdf:type owl:Class ;
         rdfs:label "目的"@zh-cn ;
         rdfs:comment "使用实体或采取行动的最终目标 (Intended Purpose) (来源: EU AI Act)。"@zh-cn .

airo:法规 rdf:type owl:Class ;
         rdfs:label "法规"@zh-cn ;
         rdfs:comment "具有约束力的立法法案 (如 EU AI Act) (来源: EU Law)。"@zh-cn .

airo:风险 rdf:type owl:Class ;
         rdfs:subClassOf airo:风险概念 ;
         rdfs:label "风险"@zh-cn ;
         rdfs:comment "与 AI 系统相关的不确定性状态，由后果的可能性和严重性组合而成 (来源: ISO 31000)。"@zh-cn .

airo:风险概念 rdf:type owl:Class ;
             rdfs:label "风险概念"@zh-cn ;
             rdfs:comment "指代风险、风险源、后果和影响的总称 (来源: ISO 31000)。"@zh-cn .

airo:风险源 rdf:type owl:Class ;
           rdfs:subClassOf airo:风险概念 ;
           rdfs:label "风险源"@zh-cn ;
           rdfs:comment "单独或组合起来具有产生风险潜力的要素 (Risk Source) (来源: ISO 31000)。"@zh-cn .

airo:严重性 rdf:type owl:Class ;
           rdfs:label "严重性"@zh-cn ;
           rdfs:comment "后果或影响的程度等级 (Severity) (来源: ISO Guide 73)。"@zh-cn .

airo:利益相关者 rdf:type owl:Class ;
               rdfs:label "利益相关者"@zh-cn ;
               rdfs:comment "能够影响决策或活动、受其影响或自认为受其影响的个人、团体或组织 (来源: ISO/IEC 22989)。"@zh-cn .

airo:标准 rdf:type owl:Class ;
           rdfs:label "标准"@zh-cn ;
           rdfs:comment "由认可的标准化机构（如 ISO/IEC, CEN/CENELEC）制定的技术规范 (来源: ISO)。"@zh-cn .

airo:威胁 rdf:type owl:Class ;
           rdfs:subClassOf airo:风险源 ;
           rdfs:label "威胁"@zh-cn ;
           rdfs:comment "危险、伤害或其他不良后果的潜在来源，通常暗示恶意意图 (Threat) (来源: ISO 27000)。"@zh-cn .

airo:版本 rdf:type owl:Class ;
           rdfs:label "版本"@zh-cn ;
           rdfs:comment "分配给 AI 系统唯一状态的唯一编号或名称 (来源: AIRO)。"@zh-cn .

airo:脆弱性 rdf:type owl:Class ;
             rdfs:label "脆弱性"@zh-cn ;
             rdfs:comment "指实体（如 AI 系统或组件）导致容易受风险源影响的弱点 (Vulnerability) (来源: ISO 27000)。"@zh-cn .

airo:风险控制措施 rdf:type owl:Class ;
                 rdfs:label "风险控制措施"@zh-cn ;
                 rdfs:comment "用于修正或维持风险的具体措施（原‘风险控制’类，现作为风险应对活动的产出） (来源: ISO 31000)。"@zh-cn .

#################################################################
#    7. 属性 (Object Properties)
#################################################################

prov:used rdf:type owl:ObjectProperty ; rdfs:label "使用了 (prov:used)"@zh-cn .
prov:generated rdf:type owl:ObjectProperty ; rdfs:label "生成了 (prov:generated)"@zh-cn .
prov:wasGeneratedBy rdf:type owl:ObjectProperty ; rdfs:label "生成自 (prov:wasGeneratedBy)"@zh-cn .
prov:wasInformedBy rdf:type owl:ObjectProperty ; rdfs:label "获知于 (prov:wasInformedBy)"@zh-cn .

###  扩展属性
airo:拥有时间要素 rdf:type owl:ObjectProperty ;
               # 域不再是AI事件，而是活动或风险
               rdfs:domain [ rdf:type owl:Class ; owl:unionOf ( airo:AI生命周期活动 airo:风险管理活动 airo:风险 ) ] ;
               rdfs:range airo:时间要素 ;
               rdfs:label "拥有时间要素"@zh-cn ;
               rdfs:comment "关联活动或风险与相关的时间线 (发生/发现/持续时间)。"@zh-cn .

airo:拥有风险等级 rdf:type owl:ObjectProperty ; rdfs:domain airo:风险 ; rdfs:range airo:风险等级 ; rdfs:label "拥有风险等级"@zh-cn ; rdfs:comment "指定风险的量化或定性等级。"@zh-cn .
airo:属于风险分类 rdf:type owl:ObjectProperty ; rdfs:domain airo:风险 ; rdfs:range airo:风险分类 ; rdfs:label "属于风险分类"@zh-cn ; rdfs:comment "指定风险所属的类别。"@zh-cn .

airo:被报道于 rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ; owl:unionOf ( airo:风险 airo:风险管理活动 ) ] ;
             rdfs:range airo:新闻报道 ;
             rdfs:label "被报道于"@zh-cn ;
             rdfs:comment "关联风险事件或活动与其原始新闻报道。"@zh-cn .

airo:由机构报道 rdf:type owl:ObjectProperty ; rdfs:domain airo:新闻报道 ; rdfs:range airo:报道机构 ; rdfs:label "由机构报道"@zh-cn ; rdfs:comment "指明新闻或报告的发布机构。"@zh-cn .

airo:拥有描述 rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ; owl:unionOf ( airo:风险 airo:风险管理活动 ) ] ;
             rdfs:range airo:详细描述 ;
             rdfs:label "拥有描述"@zh-cn ;
             rdfs:comment "关联风险或活动与其文本摘要描述。"@zh-cn .

